function celsiusToFahrenheitButton(value, type){ if(value === '') 
{ console.log("Please enter the value"); }
else
 { let convertedValue; type === 'Fahrenheit' ? convertedValue = (value - 32) * 5 / 9 : convertedValue =value * 9 / 5 + 32 console.log(value+' To '+ convertedValue) } } 


